pipeline {
    agent any

    stages {
        stage('Clone stage') {
            steps {
                git 'https://github.com/nguyenvu97/testhwebhook.git'
            }
        }
        stage('build stage') {
            steps {
               withDockerRegistry(credentialsId: 'docker-hub', url: 'https://index.docker.io/v1/') {
                sh 'docker build -t nguyenvu/golang .'
                sh 'docker push nguyenvu/golang .'
                }
            }
        }
    }
}
